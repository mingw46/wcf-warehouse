<Window x:Class="WpfApp1.InputBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="InputBox" Height="300" Width="500" Background="#668B8B">
    <Grid>
        <StackPanel>
            <Label Content="Data odebrania" FontSize="24" HorizontalAlignment="Center"/>
            <Label Content="Ustaw datę odebrania przedmiotu od klienta (domyślnie 30 dni)" HorizontalAlignment="Center" Foreground="White" />
            <Calendar x:Name="selectOccupiedDate" Margin="10" SelectedDate=""></Calendar>
        </StackPanel>
    </Grid>
</Window>
